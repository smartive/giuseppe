{
    "extends": "tslint-config-airbnb",
    "rules": {
        "function-name": false,
        "max-line-length": [
            true,
            140
        ],
        "no-increment-decrement": false,
        "ter-arrow-parens": [
            true,
            "as-needed"
        ],
        "variable-name": [
            true,
            "check-format",
            "ban-keywords",
            "allow-leading-underscore"
        ]
    }
}
