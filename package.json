{
  "name": "giuseppe",
  "version": "0.3.1",
  "description": "A controller routing system for expressJS with typescript decorators and annotations",
  "main": "index.js",
  "scripts": {
    "bootstrap": "npm install -q && typings install",
    "clean": "rimraf build",
    "precitest": "npm run bootstrap && npm run clean && tsc",
    "citest": "istanbul cover _mocha --report lcovonly -- --ui bdd --recursive ./build",
    "postcitest": "cat ./coverage/lcov.info | coveralls",
    "develop": "npm run clean && tsc --sourceMap -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/smartive/giuseppe.git"
  },
