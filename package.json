{
  "name": "giuseppe",
  "version": "1.2.0",
  "description": "An attributed controller routing system for expressJS with typescript decorators and annotations",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "bootstrap": "npm install -q",
    "build": "tsc --outDir .",
    "clean": "rimraf build coverage",
    "pretest": "npm run clean && tsc --sourceMap",
    "test": "istanbul cover -x \"**/*.spec.*\" _mocha -- --ui bdd \"./build/**/*.spec.js\"",
    "posttest": "remap-istanbul -i coverage/coverage.json -o coverage/html-report -t html -e _test",
    "precitest": "npm run bootstrap && npm run clean && tsc --sourceMap",
    "citest": "istanbul cover -x \"**/*.spec.*\" _mocha --report lcovonly -- --ui bdd \"./build/**/*.spec.js\"",
    "postcitest": "remap-istanbul -i coverage/coverage.json -o coverage/lcov-mapped.info -t lcovonly -e _test",
    "develop": "npm run clean && tsc --sourceMap -w"
  },
  "engines": {
    "node": ">=6",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/smartive/giuseppe.git"
  },
  "keywords": [
    "express",
    "routing",
    "typescript",
    "decorators",
    "api",
    "controller"
  ],
  "author": "Christoph BÃ¼hler <christoph@smartive.ch>",
  "contributors": [
    {
      "name": "Dominique Wirz",
      "email": "dominique@smartive.ch"
    },
    {
      "name": "Moreno Feltscher",
      "email": "moreno@smartive.ch"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartive/giuseppe/issues"
  },
  "homepage": "https://github.com/smartive/giuseppe#readme",
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/http-status": "^0.2.29",
    "@types/node": "^7.0.5",
    "express": "^4.14.1",
    "filewalker": "^0.1.3",
    "http-status": "^1.0.1",
    "reflect-metadata": "^0.1.9",
    "typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/mocha": "^2.2.39",
    "@types/sinon": "^1.16.34",
    "@types/sinon-chai": "^2.7.27",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "remap-istanbul": "^0.9.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "tslint": "^4.4.2",
    "tslint-jsdoc-rules": "^0.1.2"
  }
}
